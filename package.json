{
  "name": "faggot-io",
  "description": "Realtime log monitoring in your browser",
  "version": "0.4.0",
  "homepage": "https://github.com/faggot-io/faggot-io",
  "keywords": [
    "logs",
    "monitoring",
    "realtime",
    "socket.io",
    "node.js",
    "ajax"
  ],
  "license": "GPL-3.0+",
  "author": "muchweb <aleks@s-ko.net>",
  "contributors": [
    {
      "name": "Mike Smathers",
      "email": "msmathers@narrativescience.com"
    },
    {
      "name": "muchweb",
      "email": "aleks@s-ko.net"
    }
  ],
  "dependencies": {
    "backbone": "^1.1.2",
    "coffee-script": "~1.4.0",
    "express": "^4.4.4",
    "jquery": "~1.8.1",
    "jquery-browserify": "~1.8.1",
    "socket.io": "~0.9.13",
    "socket.io-client": "~0.9.11",
    "underscore": "^1.6.0",
    "winston": "^0.7.3"
  },
  "devDependencies": {
    "browserify": "~1.16.6",
    "chai": "~1.4.0",
    "coffee-script": "^1.4.0",
    "less": "~1.3.3",
    "mkdirp": "^0.5.0",
    "mocha": "~1.7.4",
    "nodeunit": "^0.9.0",
    "sinon": "~1.5.2",
    "sinon-chai": "~2.2.0",
    "yuidocjs": "^0.3.50"
  },
  "bin": {
    "faggot-io-server": "./bin/faggot-io-server.js",
    "faggot-io-harvester": "./bin/faggot-io-harvester.js"
  },
  "scripts": {
    "docs": "./node_modules/.bin/yuidoc .",
    "compile": "./node_modules/.bin/coffee --map --compile --output lib src",
    "browserify": "./node_modules/.bin/browserify lib/client.coffee -o lib/faggot-io.js --debug",
    "less": "./node_modules/.bin/lessc --compress src/less/faggot-io.less lib/faggot-io.css",
    "prepublish": "npm run compile && npm run browserify && npm run less",
    "postinstall": "cake ensure:configuration",
    "pretest": "npm run compile",
    "test": "./node_modules/.bin/nodeunit lib/test.js"
  }
}
